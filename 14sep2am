# the login screen to log into the account
def initialiseloginscreen():
    # login screen page frame

    global loginscreenframe
    loginscreenframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                              corner_radius=0, fg_color="#00FFFF",
                                              bg_color="#00FFFF")
    loginscreenframe.pack()

    #######################################################################################

    ## the back arrow frame on the login page
    #loginbackiconframe = DashboardCustomTKframe(master=loginscreenframe, width=45, height=40,
    #                                            corner_radius=10, fg_color="#00FFFF",
    #                                            bg_color="#00FFFF")
    #loginbackiconframe.place(relx=0.055, rely=0.025, anchor=NW)
    #
    ## the back arrow on the login page
    #loginbackicon = DashboardCustomTKbutton(master=loginbackiconframe,
    #                                        width=40, height=40, image=backarrowimage,
    #                                        fg_color="white", corner_radius=6,
    #                                        command=logintodashboard,
    #                                        border_width=2, hover=True)
    #loginbackicon.place(relx=0, rely=0)

    ################################################################

    # the frame for the user id entry box
    user_idframelogin = DashboardCustomTKframe(master=loginscreenframe, width=160, height=40,
                                               corner_radius=10, fg_color="#00FFFF",
                                               bg_color="#00FFFF")
    user_idframelogin.place(relx=0.32, rely=0.4)

    # the entry box for the user id  field
    user_identrylogin = customtkinter.CTkEntry(master=user_idframelogin,
                                               width=150, height=35,
                                               corner_radius=10)
    user_identrylogin.pack()

    # the frame for the user id text
    user_idtextframelogin = DashboardCustomTKframe(master=loginscreenframe,
                                                   width=20, height=40,
                                                   corner_radius=10, fg_color="White",
                                                   bg_color="#00FFFF")
    user_idtextframelogin.place(relx=0.47, rely=0.35)

    # the user id  text
    user_idtextlogin = customtkinter.CTkLabel(master=user_idtextframelogin, width=30,
                                              height=30, text="User ID")
    user_idtextlogin.pack()

    ########################

    # the frame for the error message or login successful
    logintextframe = DashboardCustomTKframe(master=loginscreenframe,
                                            width=20, height=40,
                                            corner_radius=10, fg_color="White",
                                            bg_color="#00FFFF")
    logintextframe.place(relx=0.4, rely=0.8)

    # the text for the error message or login successful
    logintext = customtkinter.CTkLabel(master=logintextframe, width=30,
                                       height=30, text="Press the button below\nto log in")
    logintext.pack()

    #################

    # the frame for the password entry box
    passwordframelogin = DashboardCustomTKframe(master=loginscreenframe, width=160, height=40,
                                                corner_radius=10, fg_color="#00FFFF",
                                                bg_color="#00FFFF")
    passwordframelogin.place(relx=0.32, rely=0.7)

    # the entry box for the first name field
    passwordentrylogin = customtkinter.CTkEntry(master=passwordframelogin,
                                                width=150, height=35,
                                                corner_radius=10)
    passwordentrylogin.pack()

    # the frame for the password text
    passwordtextframelogin = DashboardCustomTKframe(master=loginscreenframe,
                                                    width=20, height=40,
                                                    corner_radius=10, fg_color="White",
                                                    bg_color="#00FFFF")
    passwordtextframelogin.place(relx=0.43, rely=0.65)

    # the password text
    passwordtextlogin = customtkinter.CTkLabel(master=passwordtextframelogin, width=30,
                                               height=30, text="password")
    passwordtextlogin.pack()

    ##########################################################################
    # font for the top frame button
    my_font = customtkinter.CTkFont(family="Meiryo UI", size=36, weight="bold")

    # Sign up button frame
    loginsignupframe = DashboardCustomTKframe(master=loginscreenframe, width=200, height=50,
                                              corner_radius=10, fg_color="white",
                                              bg_color="white")
    loginsignupframe.place(relx=0.75, rely=0.9, anchor=CENTER)

    # a button to go to the sign in menu
    loginsignupbutton = customtkinter.CTkButton(master=loginsignupframe,
                                                width=170, height=50,
                                                text="Sign Up",
                                                corner_radius=8,
                                                command=logintosignup,
                                                fg_color="white",
                                                font=my_font,
                                                bg_color="#00FFFF",
                                                anchor=tk.CENTER,
                                                hover_color="grey",
                                                hover=True,
                                                border_color="black",
                                                border_width=2,
                                                text_color="black")
    loginsignupbutton.pack()

    # Log in Frame
    loginloginframe = DashboardCustomTKframe(master=loginscreenframe, width=200, height=50,
                                             corner_radius=10, fg_color="white",
                                             bg_color="white")
    loginloginframe.place(relx=0.25, rely=0.9, anchor=CENTER)

    ################################################################################

    def trylogin():

        loginuser_id = user_identrylogin.get()
        loginpassword = passwordentrylogin.get()
        loginuser(loginuser_id, loginpassword)

    def loginuser(loginuser_id2, loginpassword2):
        if loginuser_id2 == "":
            logintext.configure(text="User ID field is empty.")
            return False, 0, 0, "Enter a username."
        else:
            if loginpassword2 == "":
                logintext.configure(text="No password has been entered.")
                return False, 0, 0, "No password entered "
            else:
                cursor.execute("""SELECT user_id, password, experience
                            FROM user
                            WHERE user_id=(?)""", (loginuser_id2,))
                result = cursor.fetchone()
                if result is None:
                    logintext.configure(text="User ID does not exist.")
                    return False, 0, 0, "No user found "
                else:
                    if loginpassword2 == result[1]:

                        global loggedinuser
                        loggedinuser.sign_in(loginuser_id2)

                        logintext.configure(text="Logged in.\n"
                                                 "returning to dashboard")

                        logintodashboard2()

                        return True, result[1], result[2], "Sign In Successful."
                    else:
                        logintext.configure(text="Incorrect password.")
                        return False, 0, 0, "Incorrect password."

    ############################################################

    # login button.
    loginloginbutton = customtkinter.CTkButton(master=loginloginframe,
                                               width=170, height=50,
                                               text="Log In",
                                               corner_radius=8,
                                               command=trylogin,
                                               fg_color="white",
                                               font=my_font,
                                               bg_color="#00FFFF",
                                               anchor=tk.CENTER,
                                               hover_color="grey",
                                               hover=True,
                                               border_color="black",
                                               border_width=2,
                                               text_color="black")
    loginloginbutton.pack()

    window.mainloop()


# the signup screen to set up the account
def initialisesignupscreen():
    # initialising frames
    # Naming convention: [the page name][description of frame/button]

    # login screen page frame
    global signupscreenframe

    signupscreenframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                               corner_radius=0, fg_color="#00FFFF",
                                               bg_color="#00FFFF")
    signupscreenframe.pack()

    # the back arrow frame on the login page
    signupbackiconframe = DashboardCustomTKframe(master=signupscreenframe, width=45, height=40,
                                                 corner_radius=10, fg_color="#00FFFF",
                                                 bg_color="#00FFFF")
    signupbackiconframe.place(relx=0.055, rely=0.025, anchor=NW)

    # the back arrow on the signup page
    signupbackicon = DashboardCustomTKbutton(master=signupbackiconframe,
                                             width=40, height=40, image=backarrowimage,
                                             fg_color="white", corner_radius=6,
                                             command=signuptologin,
                                             border_width=2, hover=True)
    signupbackicon.place(relx=0, rely=0)

    #######################################################################

    # the frame for the first name entry box
    first_nameframe = DashboardCustomTKframe(master=signupscreenframe, width=160, height=40,
                                             corner_radius=10, fg_color="#00FFFF",
                                             bg_color="#00FFFF")
    first_nameframe.place(relx=0.32, rely=0.67)

    # the entry box for the first name field
    first_nameentry = customtkinter.CTkEntry(master=first_nameframe,
                                             width=150, height=35,
                                             corner_radius=10)
    first_nameentry.pack()

    # the frame for the First name text
    first_nametextframe = DashboardCustomTKframe(master=signupscreenframe,
                                                 width=20, height=40,
                                                 corner_radius=10, fg_color="White",
                                                 bg_color="#00FFFF")
    first_nametextframe.place(relx=0.43, rely=0.62)

    # the first name text
    first_nametext = customtkinter.CTkLabel(master=first_nametextframe, width=30,
                                            height=30, text="First name")
    first_nametext.pack()

    ######################################################################

    # the frame for the last name entry box
    last_nameframe = DashboardCustomTKframe(master=signupscreenframe, width=160, height=40,
                                            corner_radius=10, fg_color="#00FFFF",
                                            bg_color="#00FFFF")
    last_nameframe.place(relx=0.32, rely=0.5)

    # the entry box for the last name field
    last_nameentry = customtkinter.CTkEntry(master=last_nameframe,
                                            width=150, height=35,
                                            corner_radius=10)
    last_nameentry.pack()

    # the frame for the last name text
    last_nametextframe = DashboardCustomTKframe(master=signupscreenframe,
                                                width=20, height=40,
                                                corner_radius=10, fg_color="White",
                                                bg_color="#00FFFF")
    last_nametextframe.place(relx=0.43, rely=0.45)

    # the last name text
    last_nametext = customtkinter.CTkLabel(master=last_nametextframe, width=30,
                                           height=30, text="Last name")
    last_nametext.pack()

    ########################################################################

    # the frame for the confirm button to get all data
    dataconfirmframe = DashboardCustomTKframe(master=signupscreenframe, width=75, height=40,
                                              corner_radius=10, fg_color="#00FFFF",
                                              bg_color="#00FFFF")
    dataconfirmframe.place(relx=0.4, rely=0.93)

    # the command to retrieve the data from entry box

    def getdata():
        password = passwordentry.get()
        firstname = first_nameentry.get()
        lastname = last_nameentry.get()
        experience = experiencevar.get()
        userheight = signupheightslider.get()
        userweight = signupweightslider.get()
        gender = gendervar.get()
        passchecker = passwordcheck(password)
        if passchecker == 1:
            print("invalid password")
            updatetext.configure(text=" Invalid password ", fg_color="White")
        else:
            print("successful registration")

            createuser = """INSERT INTO user
                (password, first_name, last_name, experience, height, weight, gender)
                VALUES ('{}','{}','{}','{}','{}','{}','{}');""".format(password, firstname,
                                                                       lastname, experience,
                                                                       userheight, userweight,
                                                                       gender)
            cursor.execute(createuser)
            connection.commit()
            userid = cursor.lastrowid
            updatetext.configure(fg_color="White",
                                 text=" Registration successful, \n"
                                      f" Your user id code is:  \n{userid} ")

            # cursor.execute("""SELECT
            # dailycaloriein, dailycalorieout,
            # dailyproteinin, dailyfatin FROM health
            # WHERE user_id = (?)""", (userid,))
            # results = cursor.fetchone()
            # print(results)
            # print("test")
            connection.commit()
            dataconfirmbutton.pack_forget()
            backtomenubutton.pack()


    ####################################################################################

    # the button to retrieve the data
    dataconfirmbutton = DashboardCustomTKbutton(master=dataconfirmframe,
                                                width=75, height=40, text="Confirm",
                                                fg_color="White", corner_radius=8,
                                                command=getdata,
                                                border_width=0, hover=False,
                                                bg_color="#00FFFF")
    dataconfirmbutton.pack()

    backtomenubutton = DashboardCustomTKbutton(master=dataconfirmframe,
                                               width=75, height=40,
                                               text="Back to Dashboard",
                                               fg_color="White", corner_radius=8,
                                               command=signuptodashboard,
                                               border_width=0, hover=False,
                                               bg_color="#00FFFF")

    ################################################################################

    updatetextframe = DashboardCustomTKframe(master=signupscreenframe,
                                             width=80, height=40,
                                             corner_radius=10, fg_color="White",
                                             bg_color="#00FFFF")
    updatetextframe.place(relx=0.25, rely=0.72)

    # the last name text
    updatetext = customtkinter.CTkLabel(master=updatetextframe,
                                        text="", fg_color="#00FFFF",
                                        bg_color="#00FFFF")
    updatetext.pack()

    ######################################################

    gendervar = tk.StringVar()
    # the frame for the password rules
    radiobuttonsframe = DashboardCustomTKframe(master=signupscreenframe,
                                               width=60, height=60,
                                               corner_radius=10, fg_color="White",
                                               bg_color="#00FFFF")
    radiobuttonsframe.place(relx=0.42, rely=0.05)

    malebutton = tk.Radiobutton(master=radiobuttonsframe, text='Male',
                                variable=gendervar, value="Male", background="White",
                                selectcolor="#00FFFF")
    malebutton.pack()

    femalebutton = tk.Radiobutton(master=radiobuttonsframe, text='Female',
                                  variable=gendervar, value="Female", background="White",
                                  selectcolor="#00FFFF")
    femalebutton.pack()

    ####################################################################################

    # the frame for the password rules
    passwordtextframe = DashboardCustomTKframe(master=signupscreenframe,
                                               width=200, height=40,
                                               corner_radius=10, fg_color="White",
                                               bg_color="#00FFFF")
    passwordtextframe.place(relx=0.23, rely=0.15)

    # the password text
    passwordtext = customtkinter.CTkLabel(master=passwordtextframe, width=30,
                                          height=30, text="Password Entry\nMust include: 1 Uppercase, 1 Lowercase,\n"
                                                          "Over 8 Chars, One special char")
    passwordtext.pack()

    # the frame for the password entry box
    passwordentryframe = DashboardCustomTKframe(master=signupscreenframe, width=160, height=40,
                                                corner_radius=10, fg_color="#00FFFF",
                                                bg_color="#00FFFF")
    passwordentryframe.place(relx=0.32, rely=0.23)

    # the entry box for the last name field
    passwordentry = customtkinter.CTkEntry(master=passwordentryframe,
                                           width=150, height=35,
                                           corner_radius=10)
    passwordentry.pack()

    ################################################################################

    experiencesliderframe = DashboardCustomTKframe(master=signupscreenframe, width=200,
                                                   height=50, corner_radius=20,
                                                   fg_color="White", bg_color="#00FFFF")
    experiencesliderframe.place(relx=0.2, rely=0.32)

    signupheightslidertext = customtkinter.CTkLabel(master=experiencesliderframe,
                                                    text="1= Beginner, 2= Intermediate, "
                                                         "3= Comfortable")
    signupheightslidertext.pack()

    experiencevar = tk.IntVar()
    experienceslider = customtkinter.CTkSlider(master=experiencesliderframe,
                                               from_=1, to=3, variable=experiencevar,
                                               number_of_steps=2, height=15, width=180)
    experienceslider.pack()

    experiencevalue = customtkinter.CTkLabel(master=experiencesliderframe,
                                             textvariable=experiencevar)
    experiencevalue.pack()

    ############################################################################

    # sign up frame for height slider, text and updating text
    signupheightsliderframe = DashboardCustomTKframe(master=signupscreenframe, width=180,
                                                     height=50, corner_radius=20,
                                                     fg_color="White", bg_color="#00FFFF")
    signupheightsliderframe.place(relx=0.54, rely=0.8)

    signupheightslidertext = customtkinter.CTkLabel(master=signupheightsliderframe,
                                                    text="Enter Height in CM")
    signupheightslidertext.pack()

    heightvar = tk.IntVar()
    signupheightslider = customtkinter.CTkSlider(master=signupheightsliderframe,
                                                 from_=150, to=190, variable=heightvar,
                                                 number_of_steps=40, height=20, width=180)

    signupheightslider.pack()

    signupheightslidervalue = customtkinter.CTkLabel(master=signupheightsliderframe,
                                                     textvariable=heightvar)
    signupheightslidervalue.pack()

    ####################################################################################

    # sign up frame for weight slider, text and updating text
    signupweightsliderframe = DashboardCustomTKframe(master=signupscreenframe, width=180,
                                                     height=50, corner_radius=20,
                                                     fg_color="White", bg_color="#00FFFF")
    signupweightsliderframe.place(relx=0.01, rely=0.8)

    signupweightslidertext = customtkinter.CTkLabel(master=signupweightsliderframe,
                                                    text="Enter Weight in KG")
    signupweightslidertext.pack()

    weightvar = tk.IntVar()
    signupweightslider = customtkinter.CTkSlider(master=signupweightsliderframe,
                                                 from_=50, to=110, variable=weightvar,
                                                 number_of_steps=60, height=20, width=180)

    signupweightslider.pack()

    signupweightslidervalue = customtkinter.CTkLabel(master=signupweightsliderframe,
                                                     textvariable=weightvar)
    signupweightslidervalue.pack()

    ######################################################################

    window.mainloop()


# the stats at a glance frame
def initialisestatsscreen():
    # login screen page frame
    global statsscreenframe

    usercalgoal = usercaloriegoal
    usercalin = usercaloriein

    usercalburntgoal = usercalorieburntgoal
    usercalburnt = usercalorieburnt
    usercaldailyprogress = 0
    usercaldailyprogress = usercalburntgoal - usercalburnt

    userfats = userfatintake
    usersugar = usersugarintake
    userprotein = userproteinintake

    cursor.execute("""SELECT dailycaloriein, dailycalorieout, dailyproteinin, dailyfatin
    FROM health WHERE user_id = (?)""", (loggedinuser.userloggedinid,))
    results = cursor.fetchone()
    print(results)

    statsscreenframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                              corner_radius=0, fg_color="#00FFFF",
                                              bg_color="#00FFFF")
    statsscreenframe.pack()

    piechartcompositionframe = DashboardCustomTKframe(master=statsscreenframe, width=400, height=700,
                                                      corner_radius=0, fg_color="#00FFFF",
                                                      bg_color="#00FFFF")
    piechartcompositionframe.place(relx=0.3, rely=0.3)

    piechartburntframe = DashboardCustomTKframe(master=statsscreenframe, width=400, height=700,
                                                corner_radius=0, fg_color="#00FFFF",
                                                bg_color="#00FFFF")
    piechartburntframe.place(relx=0.3, rely=0.6)

    piechartintakeframe = DashboardCustomTKframe(master=statsscreenframe, width=400, height=700,
                                                 corner_radius=0, fg_color="#00FFFF",
                                                 bg_color="#00FFFF")
    piechartintakeframe.place(relx=0.3, rely=0.8)

    # the back arrow frame on the all workouts frame
    statsscreenbackarrowframe = DashboardCustomTKframe(master=statsscreenframe, width=45,
                                                       height=45, corner_radius=10,
                                                       fg_color="#00FFFF",
                                                       bg_color="#00FFFF")
    statsscreenbackarrowframe.place(relx=0.05, rely=0.005)

    # the back arrow on the all workouts frame
    statsscreenbackarrow = DashboardCustomTKbutton(master=statsscreenbackarrowframe,
                                                   width=40, height=40,
                                                   image=backarrowimage,
                                                   command=statsscreentodashboard)
    statsscreenbackarrow.pack()

    framechartslt = tk.Frame(piechartcompositionframe)
    framechartslt.pack()

    framechartslt2 = tk.Frame(piechartburntframe)
    framechartslt2.pack()

    framechartslt3 = tk.Frame(piechartintakeframe)
    framechartslt3.pack()

    nutrientlabels = ['Fats', 'Protein', 'Sugar']
    nutrientdata = np.array([userfats, usersugar, userprotein])

    usercalburntlabel = ['Daily Progress', 'Daily goal']
    usercalburntdata = np.array([usercaldailyprogress, usercalburntgoal])

    userintakelabels = ['Cals in', 'Cal Goal']
    userintakedata = np.array([usercalin, usercalgoal])

    fig = Figure()  # create a figure object
    fig2 = Figure()  # create a figure object
    fig3 = Figure()  # create a figure object

    ax = fig.add_subplot(111)  # add an Axes to the figure
    fig.patch.set_facecolor("white")  # 00FFFF
    fig.set_figwidth(2)
    fig.set_figheight(2)

    ax2 = fig2.add_subplot(111)  # add an Axes to the figure
    fig2.patch.set_facecolor("white")  # 00FFFF
    fig2.set_figwidth(2)
    fig2.set_figheight(2)

    ax3 = fig3.add_subplot(111)  # add an Axes to the figure
    fig3.patch.set_facecolor("white")  # 00FFFF
    fig3.set_figwidth(2)
    fig3.set_figheight(2)

    ax.pie(nutrientdata, radius=1, labels=nutrientlabels, autopct='%0.1f%%', shadow=True)
    ax2.pie(usercalburntdata, radius=1, labels=usercalburntlabel, autopct='%0.1f%%', shadow=True)
    ax3.pie(userintakedata, radius=1, labels=userintakelabels, autopct='%0.1f%%', shadow=True)

    chart1 = FigureCanvasTkAgg(fig, framechartslt)
    chart1.get_tk_widget().pack()

    chart2 = FigureCanvasTkAgg(fig2, framechartslt2)
    chart2.get_tk_widget().pack()

    chart3 = FigureCanvasTkAgg(fig3, framechartslt3)
    chart3.get_tk_widget().pack()


# the signup screen to set up the account
def initialisehealthdata():
    # login screen page frame
    global healthdataframe

    healthdataframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                             corner_radius=0, fg_color="#00FFFF",
                                             bg_color="#00FFFF")
    healthdataframe.pack()

    # the back arrow frame input health data
    inputhealthdatabackarrowframe = DashboardCustomTKframe(master=healthdataframe, width=45,
                                                           height=40, corner_radius=10,
                                                           fg_color="#00FFFF",
                                                           bg_color="#00FFFF")
    inputhealthdatabackarrowframe.place(relx=0.055, rely=0.025, anchor=NW)

    # the back arrow input health data
    inputhealthdatabackarrow = DashboardCustomTKbutton(master=inputhealthdatabackarrowframe,
                                                       width=40, height=40, image=backarrowimage,
                                                       fg_color="white", corner_radius=6,
                                                       command=healthdatatoallstats,
                                                       border_width=2, hover=True)
    inputhealthdatabackarrow.place(relx=0, rely=0)

    ##########

    # the frame for user fats intake
    userfatintakeentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                     corner_radius=10, fg_color="#00FFFF",
                                                     bg_color="#00FFFF")
    userfatintakeentryframe.place(relx=0.32, rely=0.05)

    # the entry box for user fats intake
    userfatintakeentry = customtkinter.CTkEntry(master=userfatintakeentryframe,
                                                width=150, height=35,
                                                corner_radius=10)
    userfatintakeentry.pack()

    # the frame for the user fats intake text
    userfatintaketextframe = DashboardCustomTKframe(master=healthdataframe,
                                                    width=20, height=40,
                                                    corner_radius=10, fg_color="White",
                                                    bg_color="#00FFFF")
    userfatintaketextframe.place(relx=0.4, rely=0.1)

    # the text for the user fats intake
    userfatintaketext = customtkinter.CTkLabel(master=userfatintaketextframe, width=30,
                                               height=30, text="Daily intake of\n"
                                                               "fats in Grams")
    userfatintaketext.pack()

    ##########

    # the frame for user protein intake
    userproteinintakeentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                         corner_radius=10, fg_color="#00FFFF",
                                                         bg_color="#00FFFF")
    userproteinintakeentryframe.place(relx=0.32, rely=0.15)

    # the entry box for user protein intake
    userproteinintakeentry = customtkinter.CTkEntry(master=userproteinintakeentryframe,
                                                    width=150, height=35,
                                                    corner_radius=10)
    userproteinintakeentry.pack()

    # the frame for the user protein intake text
    userproteinintaketextframe = DashboardCustomTKframe(master=healthdataframe,
                                                        width=20, height=40,
                                                        corner_radius=10, fg_color="White",
                                                        bg_color="#00FFFF")
    userproteinintaketextframe.place(relx=0.4, rely=0.2)

    # the text for the user protein intake
    userfatintaketext = customtkinter.CTkLabel(master=userproteinintaketextframe, width=30,
                                               height=30, text="Daily intake of\n"
                                                               "protein in Grams")
    userfatintaketext.pack()

    ##########

    # the frame for user sugar intake
    usersugarintakeentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                       corner_radius=10, fg_color="#00FFFF",
                                                       bg_color="#00FFFF")
    usersugarintakeentryframe.place(relx=0.32, rely=0.25)

    # the entry box for user sugar intake
    usersugarintakeentry = customtkinter.CTkEntry(master=usersugarintakeentryframe,
                                                  width=150, height=35,
                                                  corner_radius=10)
    usersugarintakeentry.pack()

    # the frame for the user sugar intake text
    usersugarintaketextframe = DashboardCustomTKframe(master=healthdataframe,
                                                      width=20, height=40,
                                                      corner_radius=10, fg_color="White",
                                                      bg_color="#00FFFF")
    usersugarintaketextframe.place(relx=0.4, rely=0.30)

    # the text for the user sugar intake
    usersugarintaketext = customtkinter.CTkLabel(master=usersugarintaketextframe, width=30,
                                                 height=30, text="Daily intake of\n"
                                                                 "sugar in Grams")
    usersugarintaketext.pack()

    ##########

    # the frame for user daily calorie intake
    usercalorieintakeentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                         corner_radius=10, fg_color="#00FFFF",
                                                         bg_color="#00FFFF")
    usercalorieintakeentryframe.place(relx=0.32, rely=0.4)

    # the entry box for daily calorie intake
    usercalorieintakeentry = customtkinter.CTkEntry(master=usercalorieintakeentryframe,
                                                    width=150, height=35,
                                                    corner_radius=10)
    usercalorieintakeentry.pack()

    # the frame for the user daily calorie intake
    dailycalorieintaketextframe = DashboardCustomTKframe(master=healthdataframe,
                                                         width=20, height=40,
                                                         corner_radius=10, fg_color="White",
                                                         bg_color="#00FFFF")
    dailycalorieintaketextframe.place(relx=0.4, rely=0.45)

    # the text for the user daily calorie intake
    dailycalorieintaketext = customtkinter.CTkLabel(master=dailycalorieintaketextframe, width=30,
                                                    height=30, text="Daily calorie\n"
                                                                    "intake")
    dailycalorieintaketext.pack()

    ########

    # the frame for user daily calorie target
    usercalorietargetentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                         corner_radius=10, fg_color="#00FFFF",
                                                         bg_color="#00FFFF")
    usercalorietargetentryframe.place(relx=0.32, rely=0.5)

    # the entry box for daily calorie target
    usercalorietargetentry = customtkinter.CTkEntry(master=usercalorietargetentryframe,
                                                    width=150, height=35,
                                                    corner_radius=10)
    usercalorietargetentry.pack()

    # the frame for the user daily calorie target
    dailycalorietargettextframe = DashboardCustomTKframe(master=healthdataframe,
                                                         width=20, height=40,
                                                         corner_radius=10, fg_color="White",
                                                         bg_color="#00FFFF")
    dailycalorietargettextframe.place(relx=0.4, rely=0.55)

    # the text for the user daily calorie target
    dailycalorietargettext = customtkinter.CTkLabel(master=dailycalorietargettextframe, width=30,
                                                    height=30, text="Daily calorie\n"
                                                                    "target")
    dailycalorietargettext.pack()

    ########

    # the frame for user daily calorie burnt goal
    usercalorieburntgoalentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                            corner_radius=10, fg_color="#00FFFF",
                                                            bg_color="#00FFFF")
    usercalorieburntgoalentryframe.place(relx=0.32, rely=0.65)

    # the entry for user daily calorie burnt goal
    usercalorieburntgoalentry = customtkinter.CTkEntry(master=usercalorieburntgoalentryframe,
                                                       width=150, height=35,
                                                       corner_radius=10)
    usercalorieburntgoalentry.pack()

    # the frame for user daily calorie burnt goal text
    dailycalorieburntgoaltextframe = DashboardCustomTKframe(master=healthdataframe,
                                                            width=20, height=40,
                                                            corner_radius=10, fg_color="White",
                                                            bg_color="#00FFFF")
    dailycalorieburntgoaltextframe.place(relx=0.4, rely=0.70)

    # the user daily calorie burnt goal text
    dailycalorieburntgoaltext = customtkinter.CTkLabel(master=dailycalorieburntgoaltextframe, width=30,
                                                       height=30, text="Daily calorie\n"
                                                                       "Burnt Goal")
    dailycalorieburntgoaltext.pack()

    ######

    # the frame for user daily calorie burnt
    usercalorieburntentryframe = DashboardCustomTKframe(master=healthdataframe, width=160, height=40,
                                                        corner_radius=10, fg_color="#00FFFF",
                                                        bg_color="#00FFFF")
    usercalorieburntentryframe.place(relx=0.32, rely=0.75)

    # the entry for user daily calorie burnt
    usercalorieburntentry = customtkinter.CTkEntry(master=usercalorieburntentryframe,
                                                   width=150, height=35,
                                                   corner_radius=10)
    usercalorieburntentry.pack()

    # the frame for user daily calorie burnt text
    dailycalorieburnttextframe = DashboardCustomTKframe(master=healthdataframe,
                                                        width=20, height=40,
                                                        corner_radius=10, fg_color="White",
                                                        bg_color="#00FFFF")
    dailycalorieburnttextframe.place(relx=0.4, rely=0.8)

    # the user daily calorie burnt text
    dailycalorieburnttext = customtkinter.CTkLabel(master=dailycalorieburnttextframe, width=30,
                                                   height=30, text="Daily calorie\n"
                                                                   "Burnt")
    dailycalorieburnttext.pack()

    ######

    # the frame for the confirm button to get all data
    healthdataconfirmframe = DashboardCustomTKframe(master=healthdataframe, width=100, height=50,
                                                    corner_radius=10, fg_color="#00FFFF",
                                                    bg_color="#00FFFF")
    healthdataconfirmframe.place(relx=0.4, rely=0.9)

    def gethealthdata():

        global useridsignin
        global userproteinintake
        global userfatintake
        global usersugarintake
        global usercaloriegoal
        global usercaloriein
        global usercalorieburntgoal
        global usercalorieburnt

        useridsignin = loggedinuser.userloggedinid
        userproteinintake = userproteinintakeentry.get()
        userfatintake = userfatintakeentry.get()
        usersugarintake = usersugarintakeentry.get()
        usercaloriegoal = usercalorietargetentry.get()
        usercaloriein = usercalorieintakeentry.get()
        usercalorieburntgoal = usercalorieburntgoalentry.get()
        usercalorieburnt = usercalorieburntentry.get()

        addhealthdata = """INSERT INTO health
                        (user_id, dailycaloriein, dailycaloriegoal, dailyproteinin,
                        dailyfatin, dailysugarin, dailycalorieburntgoal, 
                        dailycalorieburnt)
                        VALUES ('{}','{}','{}','{}','{}','{}','{}','{}');""".format(useridsignin,
                                                                                    usercaloriein,
                                                                                    usercaloriegoal,
                                                                                    userproteinintake,
                                                                                    userfatintake,
                                                                                    usersugarintake,
                                                                                    usercalorieburntgoal,
                                                                                    usercalorieburnt)
        cursor.execute(addhealthdata)
        connection.commit()

    # the button to retrieve the health data
    healthdataconfirmbutton = DashboardCustomTKbutton(master=healthdataconfirmframe,
                                                      width=75, height=40, text="Confirm",
                                                      fg_color="White", corner_radius=8,
                                                      command=gethealthdata, border_width=0,
                                                      hover=False, bg_color="#00FFFF")
    healthdataconfirmbutton.pack()

    window.mainloop()


# the commands to go from one page to the other.
# command to go from dashboard to workout
def dashboardtoworkout():
    dashboardframe.pack_forget()
    initialiseworkouts()


# command to go from dashboard to health
def dashboardtohealth():
    dashboardframe.pack_forget()
    initialisehealth()


# command to go from dashboard to health
def dashboardtoallrecipes():
    dashboardframe.pack_forget()
    initialiseallrecipes()


# command to go from dashboard to health
def dashboardtoallworkouts():
    dashboardframe.pack_forget()
    initialiseallworkouts()


# command to go from workout to dashboard
def workouttodashboard():
    workoutframe.pack_forget()
    initialisedashboard()


# command to go from workout to health
def workouttohealth():
    workoutframe.pack_forget()
    initialisehealth()


# command to go from health to dashboard
def healthtodashboard():
    healthframe.pack_forget()
    initialisedashboard()


# command to go from health to workout
def healthtoworkout():
    healthframe.pack_forget()
    initialiseworkouts()


# command to go from health to workout
def dashboardtologin():
    dashboardframe.pack_forget()
    initialiseloginscreen()


# the command to go from the login to the dashboard
def logintodashboard():
    loginscreenframe.pack_forget()
    initialisedashboard()


