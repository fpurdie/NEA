window.resizable(False, False)
window.configure(bg='#00FFFF')

wipeusers = "DELETE FROM user;"


# class for my custom build frames.
class DashboardCustomTKframe(customtkinter.CTkFrame):
    def __init__(self, master, width=350, height=150, corner_radius=10, fg_color="white",
                 bg_color="white"):
        super().__init__(master)
        self.configure(width=width, height=height, corner_radius=corner_radius,
                       fg_color=fg_color, bg_color=bg_color)


# this is used when a button servers no use
def onclick():
    print("This is used when a button has no purpose, \nlike pressing a tooltip button which is already pressed.")


# class for my custom build buttons.
class DashboardCustomTKbutton(customtkinter.CTkButton):
    def __init__(self, master, width=150, height=150,
                 fg_color="white", text="", command=onclick, text_color="black",
                 border_width=2, corner_radius=8, bg_color="#00FFFF",
                 image=None, hover_color="grey",
                 hover=True, *args, **kwargs):
        super().__init__(master, width=width, height=height,
                         fg_color=fg_color, hover_color=hover_color,
                         hover=hover, text=text, command=command,
                         border_width=border_width, bg_color=bg_color,
                         corner_radius=corner_radius, text_color=text_color,
                         image=image)


# class for signing in a user
class SignInUser:
    def __init__(self):
        self.userloggedinid = 0
        print(self.userloggedinid)

    def get_id(self):
        return self.userloggedinid

    def sign_in(self, idvariable):
        self.userloggedinid = idvariable

    def sign_out(self):
        self.userloggedinid = 0

        sign_in_info = SignInUser()


# checking a password
def passwordcheck(password):
    if len(password) < 8:
        error = 1
        return error
        # raise sqlite3.IntegrityError('Password must be over 8 characters long.')
    elif not re.search(r"[a-z]", password):
        error = 1
        return error
        # raise sqlite3.IntegrityError('Password must have at least 1 lowercase character.')
    elif not re.search(r"[A-Z]", password):
        error = 1
        return error
        # raise sqlite3.IntegrityError('Password must have at least 1 uppercase character.')
    elif not re.search(r"\d", password):
        error = 1
        return error
        # raise sqlite3.IntegrityError('Password must have at least 1 number.')
    elif not re.search(r"[^a-zA-Z0-9]", password):
        error = 1
        return error
        # raise sqlite3.IntegrityError('Password must have at least 1 special character.')
    else:
        return 0


# used to exit program.
def quitprogram():
    quit()


loggedinid = 0
loggedinuser = SignInUser()
loggedinuser.sign_in(loggedinid)


# will be used to open dashboard
def initialisedashboard():
    # hiding frames from other pages
    # initialising frames
    # Naming convention: [the page name][description of frame/button]
    # dashboard page frame
    global dashboardframe
    dashboardframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                            corner_radius=0, fg_color="#00FFFF",
                                            bg_color="#00FFFF")
    dashboardframe.pack()

    # dashboard user icon frame
    dashboardusericonframe = DashboardCustomTKframe(master=dashboardframe, width=45, height=40,
                                                    corner_radius=10, fg_color="#00FFFF",
                                                    bg_color="#00FFFF")
    dashboardusericonframe.place(relx=0.9, rely=0.035, anchor=NE)

    # dashboard user icon
    dashboardusericon = DashboardCustomTKbutton(master=dashboardusericonframe,
                                                width=40, height=40, image=usericonimage,
                                                fg_color="white", corner_radius=6,
                                                command=dashboardtologin,
                                                border_width=2, hover=True)
    #dashboardusericon.place(relx=0, rely=0)

    # dashboard quit program frame
    dashboardquiticonframe = DashboardCustomTKframe(master=dashboardframe, width=45, height=40,
                                                    corner_radius=10, fg_color="#00FFFF",
                                                    bg_color="#00FFFF")
    dashboardquiticonframe.place(relx=0.1, rely=0.035, anchor=NW)

    # dashboard quit program icon
    dashboardquiticon = DashboardCustomTKbutton(master=dashboardquiticonframe,
                                                width=40, height=40, image=quiticonimage,
                                                fg_color="white", corner_radius=6,
                                                command=quitprogram,
                                                border_width=2, hover=True)
    dashboardquiticon.place(relx=0, rely=0)

    # dashboard title frame
    dashboardtextframe = DashboardCustomTKframe(master=dashboardframe, width=155, height=45,
                                                corner_radius=10, fg_color="white",
                                                bg_color="#00FFFF")
    dashboardtextframe.place(relx=0.5, rely=0.03, anchor=N)
    # dashboard title text
    dashboardtitletext = customtkinter.CTkLabel(master=dashboardtextframe,
                                                text="WorkoutPal",
                                                text_color="black",
                                                width=120,
                                                height=25,
                                                font=customtkinter.CTkFont(family="Meiryo UI",
                                                                           weight="bold",
                                                                           size=22),
                                                corner_radius=8)

    dashboardtitletext.place(relx=0.5, rely=0.5, anchor=CENTER)

    # dashboard view stats frame
    dashboardviewstatsframe = DashboardCustomTKframe(master=dashboardframe, width=350, height=150,
                                                     corner_radius=10, fg_color="white",
                                                     bg_color="#00FFFF")
    dashboardviewstatsframe.place(relx=0.5, rely=0.13, anchor=N)

    # dashboard view stats button
    dashboardviewstatsbutton = DashboardCustomTKbutton(master=dashboardviewstatsframe,
                                                       width=20, height=22,
                                                       image=statsataglanceimage,
                                                       fg_color="#00FFFF", corner_radius=8,
                                                       command=dashboardtostatsscreen,
                                                       border_width=0, hover=False,
                                                       bg_color="#00FFFF")
    dashboardviewstatsbutton.pack()

    # dashboard view saved recipes frame
    dashboardviewsavedrecipesframe = DashboardCustomTKframe(master=dashboardframe, width=350, height=150,
                                                            corner_radius=10, fg_color="white",
                                                            bg_color="#00FFFF")
    dashboardviewsavedrecipesframe.place(relx=0.5, rely=0.385, anchor=N)

    # dashboard view saved recipes button
    dashboardviewsavedrecipesbutton = DashboardCustomTKbutton(master=dashboardviewsavedrecipesframe,
                                                              width=20, height=22,
                                                              image=savedrecipesimage,
                                                              fg_color="#00FFFF", corner_radius=8,
                                                              command=dashboardtoallrecipes,
                                                              border_width=0, hover=False,
                                                              bg_color="#00FFFF")
    dashboardviewsavedrecipesbutton.pack()

    # dashboard view suggested workouts frame
    dashboardviewsuggestedworkoutsframe = DashboardCustomTKframe(master=dashboardframe,
                                                                 width=350, height=150,
                                                                 corner_radius=10, fg_color="white",
                                                                 bg_color="#00FFFF")
    dashboardviewsuggestedworkoutsframe.place(relx=0.5, rely=0.65, anchor=N)

    # dashboard view suggested workout button
    dashboardviewsuggestedworkoutsbutton = DashboardCustomTKbutton(master=dashboardviewsuggestedworkoutsframe,
                                                                   width=20, height=22,
                                                                   image=suggestedworkoutsimage,
                                                                   fg_color="#00FFFF", corner_radius=8,
                                                                   command=dashboardtoallworkouts,
                                                                   border_width=0, hover=False,
                                                                   bg_color="#00FFFF")
    dashboardviewsuggestedworkoutsbutton.pack()

    # dashboard bottom frame
    dashboardbottomframe = DashboardCustomTKframe(master=dashboardframe, width=400, height=80,
                                                  corner_radius=10, fg_color="white",
                                                  bg_color="#00FFFF")
    dashboardbottomframe.place(relx=0.5, rely=0.96, anchor=CENTER)

    # dashboard toolbar frames
    # workout icon to take to work out section
    dashboardworkoutframe = DashboardCustomTKframe(master=dashboardbottomframe,
                                                   width=70, height=70,
                                                   corner_radius=10, fg_color="white",
                                                   bg_color="white")
    dashboardworkoutframe.place(relx=0.08, rely=0.1)

    # the button to take you to work out section
    dashboardworkoutbutton = DashboardCustomTKbutton(master=dashboardworkoutframe,
                                                     width=50, height=50, image=workoutsimage,
                                                     fg_color="white", corner_radius=6,
                                                     command=dashboardtoworkout,
                                                     border_width=2, hover=True,
                                                     bg_color="white")
    dashboardworkoutbutton.place(relx=0, rely=0)

    # dashboard icon used to return to dashboard, on by default.
    dashboarddashboardframe = DashboardCustomTKframe(master=dashboardbottomframe,
                                                     width=70, height=70,
                                                     corner_radius=10, fg_color="#00FFFF",
                                                     bg_color="white")
    dashboarddashboardframe.place(relx=0.42, rely=0.1)

    # the button to return to dashboard
    dashboarddashboardbutton = DashboardCustomTKbutton(master=dashboarddashboardframe,
                                                       width=70, height=70,
                                                       image=dashboardimageclicked,
                                                       fg_color="white", corner_radius=6,
                                                       command=onclick,
                                                       border_width=2, hover=True,
                                                       bg_color="white")
    dashboarddashboardbutton.place(relx=0, rely=0)

    # health icon button frame
    dashboardhealthframe = DashboardCustomTKframe(master=dashboardbottomframe,
                                                  width=70, height=70,
                                                  corner_radius=10, fg_color="#00FFFF",
                                                  bg_color="white")
    dashboardhealthframe.place(relx=0.75, rely=0.1)

    # health button to take to health section
    dashboardhealthbutton = DashboardCustomTKbutton(master=dashboardhealthframe,
                                                    width=70, height=70,
                                                    image=healthimage,
                                                    fg_color="white", corner_radius=6,
                                                    command=dashboardtohealth,
                                                    border_width=2, hover=True,
                                                    bg_color="white")
    dashboardhealthbutton.place(relx=0, rely=0)

    window.mainloop()


# will be used to open the workouts page.
def initialiseworkouts():
    # the workout window frame
    global workoutframe
    workoutframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                          corner_radius=0, fg_color="#00FFFF",
                                          bg_color="#00FFFF")
    workoutframe.pack()

    # the main frame for the workout page
    workoutmainframe = DashboardCustomTKframe(master=workoutframe, width=400, height=700,
                                              corner_radius=0, fg_color="#00FFFF",
                                              bg_color="#00FFFF")
    workoutmainframe.pack()

    # workouts title frame
    workoutstextframe = DashboardCustomTKframe(master=workoutmainframe, width=155, height=45,
                                               corner_radius=10, fg_color="white",
                                               bg_color="#00FFFF")
    workoutstextframe.place(relx=0.5, rely=0.02, anchor=N)
    # dashboard title text
    workoutstitletext = customtkinter.CTkLabel(master=workoutstextframe,
                                               text="WorkoutPal",
                                               text_color="black",
                                               width=120,
                                               height=25,
                                               font=customtkinter.CTkFont(family="Meiryo UI",
                                                                          weight="bold",
                                                                          size=22),
                                               corner_radius=8)

    workoutstitletext.place(relx=0.5, rely=0.5, anchor=CENTER)

    # Quit button in the top left
    # DashboardQuitIconFrame = DashboardCustomTKframe(master=WorkoutFrame, width=45, height=40,
    #                                                 corner_radius=10, fg_color="#00FFFF",
    #                                                 bg_color="#00FFFF")
    # DashboardQuitIconFrame.place(relx=0.03, rely=0.023, anchor=NW)
    #
    # # workout quit program icon
    # DashboardQuitIcon = DashboardCustomTKbutton(master=DashboardQuitIconFrame,
    #                                             width=40, height=40, image=quiticonimage,
    #                                             fg_color="white", corner_radius=6,
    #                                             command=quitprogram,
    #                                             border_width=2, hover=True)
    # DashboardQuitIcon.place(relx=0, rely=0)

    # Workout View all workouts frame
    workoutviewallworkoutsframe = DashboardCustomTKframe(master=workoutmainframe, width=350, height=250,
                                                         corner_radius=10, fg_color="white",
                                                         bg_color="#00FFFF")
    workoutviewallworkoutsframe.place(relx=0.05, rely=0.08)

    # Workout View all workouts button
    workoutviewallworkoutsbutton = DashboardCustomTKbutton(master=workoutviewallworkoutsframe,
                                                           width=20, height=22,
                                                           image=viewallworkoutsimage,
                                                           fg_color="#00FFFF", corner_radius=8,
                                                           command=workouttoworkouts,
                                                           border_width=0, hover=False, bg_color="#00FFFF")
    workoutviewallworkoutsbutton.pack()

    # Workout View suggested workouts frame
    workoutviewsuggestedworkoutsframe = DashboardCustomTKframe(master=workoutmainframe, width=350,
                                                               height=250, corner_radius=10,
                                                               fg_color="white", bg_color="#00FFFF")
    workoutviewsuggestedworkoutsframe.place(relx=0.5, rely=0.47, anchor=N)

    # Workout View suggested workouts button
    workoutviewsuggestedworkoutsbutton = DashboardCustomTKbutton(master=workoutviewsuggestedworkoutsframe,
                                                                 width=20, height=22, image=suggestedworkouts2image,
                                                                 fg_color="#00FFFF", corner_radius=8,
                                                                 command=workouttohealthdata,
                                                                 border_width=0, hover=False, bg_color="#00FFFF")
    workoutviewsuggestedworkoutsbutton.pack()

    # bottom toolbar for workouts
    workoutbottomframe = DashboardCustomTKframe(master=workoutmainframe, width=400,
                                                height=80, corner_radius=10,
                                                fg_color="white", bg_color="#00FFFF")
    workoutbottomframe.place(relx=0.5, rely=0.96, anchor=CENTER)

    # bottom left frame in bottom frame, highlighted as workout to indicate current tab
    workoutworkoutframe = DashboardCustomTKframe(master=workoutbottomframe, width=70, height=70,
                                                 corner_radius=10, fg_color="white",
                                                 bg_color="white")
    workoutworkoutframe.place(relx=0.08, rely=0.1)

    # workout button on the toolbar, sole purpose is not to do anything only to indicate tab
    workoutworkoutbutton = DashboardCustomTKbutton(master=workoutworkoutframe,
                                                   width=50, height=50,
                                                   image=workoutsimageclicked,
                                                   fg_color="white", corner_radius=6,
                                                   border_width=2, hover=True,
                                                   bg_color="white")
    workoutworkoutbutton.place(relx=0, rely=0)

    # dashboard frame storing icon used to return to dashboard
    workoutdashboardframe = DashboardCustomTKframe(master=workoutbottomframe,
                                                   width=70, height=70,
                                                   corner_radius=10, fg_color="#00FFFF",
                                                   bg_color="white")
    workoutdashboardframe.place(relx=0.42, rely=0.1)

    # dashboard icon used to return to dashboard
    workoutdashboardbutton = DashboardCustomTKbutton(master=workoutdashboardframe,
                                                     width=70, height=70,
                                                     image=dashboardimage,
                                                     fg_color="white", corner_radius=6,
                                                     command=workouttodashboard,
                                                     border_width=2, hover=True,
                                                     bg_color="white")
    workoutdashboardbutton.place(relx=0, rely=0)

    # health frame containing icon to take to health section
    workouthealthframe = DashboardCustomTKframe(master=workoutbottomframe, width=70, height=70,
                                                corner_radius=10, fg_color="#00FFFF",
                                                bg_color="white")
    workouthealthframe.place(relx=0.75, rely=0.1)

    # health icon to take to health section
    workouthealthbutton = DashboardCustomTKbutton(master=workouthealthframe,
                                                  width=70, height=70, image=healthimage,
                                                  fg_color="white", corner_radius=6,
                                                  command=workouttohealth,
                                                  border_width=2, hover=True, bg_color="white")
    workouthealthbutton.place(relx=0, rely=0)

    window.mainloop()


# will be used to open the health page.
def initialisehealth():
    global healthframe
    # the frame for the health page
    healthframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                         corner_radius=0, fg_color="#00FFFF",
                                         bg_color="#00FFFF")
    healthframe.pack()

    ##############################################

    # the main frame for health where all related frames are mastered
    mainhealthframe = DashboardCustomTKframe(master=healthframe, width=400, height=700,
                                             corner_radius=0, fg_color="#00FFFF",
                                             bg_color="#00FFFF")
    mainhealthframe.pack()

    ################################

    # the frame for the quit icon in top left
    healthquiticonframe = DashboardCustomTKframe(master=mainhealthframe, width=45, height=40,
                                                 corner_radius=10, fg_color="#00FFFF",
                                                 bg_color="#00FFFF")
    healthquiticonframe.place(relx=0.07, rely=0.023, anchor=NW)

    # dashboard quit program icon
    healthquiticon = DashboardCustomTKbutton(master=healthquiticonframe,
                                             width=40, height=40, image=quiticonimage,
                                             fg_color="white", corner_radius=6,
                                             command=quitprogram,
                                             border_width=2, hover=True)
    healthquiticon.place(relx=0, rely=0)

    # health title frame
    healthtextframe = DashboardCustomTKframe(master=mainhealthframe, width=155, height=45,
                                             corner_radius=10, fg_color="white",
                                             bg_color="#00FFFF")
    healthtextframe.place(relx=0.5, rely=0.02, anchor=N)
    # dashboard title text
    healthtitletext = customtkinter.CTkLabel(master=healthtextframe,
                                             text="WorkoutPal",
                                             text_color="black",
                                             width=120,
                                             height=25,
                                             font=customtkinter.CTkFont(family="Meiryo UI",
                                                                        weight="bold",
                                                                        size=22),
                                             corner_radius=8)

    healthtitletext.place(relx=0.5, rely=0.5, anchor=CENTER)

    # Health view stats frame
    healthviewdailystatsframe = DashboardCustomTKframe(master=mainhealthframe, width=350, height=150,
                                                       corner_radius=10, fg_color="white",
                                                       bg_color="#00FFFF")

    healthviewdailystatsframe.place(relx=0.5, rely=0.09, anchor=N)

    # health view stats button
    healthviewdailystatsbutton = DashboardCustomTKbutton(master=healthviewdailystatsframe,
                                                         width=20, height=22, image=viewdailystatsimage,
                                                         fg_color="#00FFFF", corner_radius=8,
                                                         command=healthtostatsscreen, border_width=0,
                                                         hover=False, bg_color="#00FFFF")
    healthviewdailystatsbutton.pack()

    # health view all recipes frame
    healthviewallrecipesframe = DashboardCustomTKframe(master=mainhealthframe, width=350, height=150,
                                                       corner_radius=10, fg_color="white",
                                                       bg_color="#00FFFF")
    healthviewallrecipesframe.place(relx=0.5, rely=0.36, anchor=N)

    # health view all recipes button
    healthviewallrecipesbutton = DashboardCustomTKbutton(master=healthviewallrecipesframe,
                                                         width=20, height=22, image=viewallrecipesimage,
                                                         fg_color="#00FFFF", corner_radius=8,
                                                         command=healthtorecipes, border_width=0,
                                                         hover=False, bg_color="#00FFFF")
    healthviewallrecipesbutton.pack()

    # health view saved recipes frame
    healthviewsavedrecipesframe = DashboardCustomTKframe(master=mainhealthframe, width=350, height=150,
                                                         corner_radius=10, fg_color="white",
                                                         bg_color="#00FFFF")
    healthviewsavedrecipesframe.place(relx=0.5, rely=0.64, anchor=N)

    # health view all recipes button
    healthviewsavedrecipesbutton = DashboardCustomTKbutton(master=healthviewsavedrecipesframe,
                                                           width=20, height=22, image=manuallylogcalories,
                                                           fg_color="#00FFFF", corner_radius=8,
                                                           command=healthtohealthdata, border_width=0,
                                                           hover=False, bg_color="#00FFFF")
    healthviewsavedrecipesbutton.pack()

    # bottom toolbar for workouts
    healthbottomframe = DashboardCustomTKframe(master=mainhealthframe, width=400,
                                               height=80, corner_radius=10,
                                               fg_color="white", bg_color="#00FFFF")
    healthbottomframe.place(relx=0.5, rely=0.96, anchor=CENTER)

    # bottom left frame in bottom frame, highlighted as workout to indicate current tab
    healthworkoutframe = DashboardCustomTKframe(master=healthbottomframe, width=70, height=70,
                                                corner_radius=10, fg_color="white",
                                                bg_color="white")
    healthworkoutframe.place(relx=0.08, rely=0.1)

    # health button on the toolbar, sole purpose is not to do anything only to indicate tab
    healthworkoutbutton = DashboardCustomTKbutton(master=healthworkoutframe,
                                                  width=50, height=50,
                                                  image=workoutsimage,
                                                  command=healthtoworkout,
                                                  fg_color="white", corner_radius=6,
                                                  border_width=2, hover=True,
                                                  bg_color="white")
    healthworkoutbutton.place(relx=0, rely=0)

    # dashboard frame storing icon used to return to dashboard
    healthdashboardframe = DashboardCustomTKframe(master=healthbottomframe,
                                                  width=70, height=70,
                                                  corner_radius=10, fg_color="#00FFFF",
                                                  bg_color="white")
    healthdashboardframe.place(relx=0.42, rely=0.1)

    # dashboard icon used to return to dashboard
    healthdashboardbutton = DashboardCustomTKbutton(master=healthdashboardframe,
                                                    width=70, height=70,
                                                    image=dashboardimage,
                                                    fg_color="white", corner_radius=6,
                                                    command=healthtodashboard,
                                                    border_width=2, hover=True,
                                                    bg_color="white")
    healthdashboardbutton.place(relx=0, rely=0)

    # health frame containing icon to take to health section
    healthhealthframe = DashboardCustomTKframe(master=healthbottomframe, width=70, height=70,
                                               corner_radius=10, fg_color="#00FFFF",
                                               bg_color="white")
    healthhealthframe.place(relx=0.75, rely=0.1)

    # health icon to take to health section
    healthhealthbutton = DashboardCustomTKbutton(master=healthhealthframe,
                                                 width=70, height=70, image=healthimageclicked,
                                                 fg_color="white", corner_radius=6,
                                                 command=onclick,
                                                 border_width=2, hover=True, bg_color="white")
    healthhealthbutton.place(relx=0, rely=0)

    window.mainloop()


# the command to go to all recipes
def initialiseallrecipes():
    # the frame for the all recipes page
    global allrecipesframe
    allrecipesframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                             corner_radius=0, fg_color="#00FFFF",
                                             bg_color="#00FFFF")
    allrecipesframe.pack()

    mainframeallrecipes = DashboardCustomTKframe(master=allrecipesframe, width=400, height=700,
                                                 corner_radius=0, fg_color="#00FFFF",
                                                 bg_color="#00FFFF")
    mainframeallrecipes.pack()

    # the function to add calories
    # def addkcal(value):
    #    dailycal = value
    #    createuser = """INSERT INTO health
    #                    (user_id, dailycaloriein)
    #                    VALUES ('{}','{}';""".format(userid, dailycal)
    #    cursor.execute(createuser)
    #    connection.commit()
    #    print("insertion complete")

    def onerecipetoall():
        onerecipeframe.pack_forget()
        mainframeallrecipes.pack()

    def steakbowlbutton():
        mainframeallrecipes.pack_forget()

        global onerecipeframe
        onerecipeframe = DashboardCustomTKframe(master=allrecipesframe, width=400, height=700,
                                                corner_radius=0, fg_color="#00FFFF",
                                                bg_color="#00FFFF")
        onerecipeframe.pack()

        backtoallrecipesarrowframe = DashboardCustomTKframe(master=onerecipeframe, width=45,
                                                            height=40, corner_radius=10,
                                                            fg_color="#00FFFF",
                                                            bg_color="#00FFFF")
        backtoallrecipesarrowframe.place(relx=0.05, rely=0.005)

        # the back arrow on the all recipes frame
        backtoallrecipesarrow = DashboardCustomTKbutton(master=backtoallrecipesarrowframe,
                                                        width=40, height=40, image=backarrowimage,
                                                        fg_color="white", corner_radius=6,
                                                        command=onerecipetoall,
                                                        border_width=2, hover=True)
        backtoallrecipesarrow.place(relx=0, rely=0)

        # the frame for the image of the food
