                                                 image=steakbowl, command=onclick,
                                                 hover=False)
        onerecipeimage.pack()

        # the frame for the name of the food
        onerecipetextitemnameframe = DashboardCustomTKframe(master=onerecipeframe,
                                                            width=20, height=40,
                                                            corner_radius=10, fg_color="White",
                                                            bg_color="#00FFFF")
        onerecipetextitemnameframe.place(relx=0.2, rely=0.025)

        # the text for the name of the food
        onerecipetext = customtkinter.CTkLabel(master=onerecipetextitemnameframe, width=30,
                                               height=30, text="Steak burrito bowl")
        onerecipetext.pack()

        # the frame for the nutritional values
        onerecipetextnutritonframe = DashboardCustomTKframe(master=onerecipeframe,
                                                            width=20, height=40,
                                                            corner_radius=10, fg_color="White",
                                                            bg_color="#00FFFF")
        onerecipetextnutritonframe.place(relx=0.22, rely=0.1)

        # the text for the nutritional values
        onerecipetextnutriton = customtkinter.CTkLabel(master=onerecipetextnutritonframe, width=30,
                                                       height=30, text="549 Kcal\n32g protein\n"
                                                                       "10g sugars\n24g fats")
        onerecipetextnutriton.pack()

        # the frame for the ingredients
        onerecipetextingredientsframe = DashboardCustomTKframe(master=onerecipeframe,
                                                               width=20, height=40,
                                                               corner_radius=10, fg_color="White",
                                                               bg_color="#00FFFF")
        onerecipetextingredientsframe.place(relx=0.025, rely=0.2)

        # the text for the ingredients
        onerecipetextingredients = customtkinter.CTkLabel(master=onerecipetextingredientsframe, width=30,
                                                          height=30, text="1 fillet steak 250g\n"
                                                                          "1 sachet fajita seasoning\n"
                                                                          "2 tbsp olive oil\n"
                                                                          "250g Mexican-style rice pouch\n"
                                                                          "2 roasted red peppers, sliced\n"
                                                                          "1 lemon, ½ juiced, ½ cut into wedges\n"
                                                                          "½ small pack chopped parsley\n"
                                                                          "yogurt, to serve")
        onerecipetextingredients.pack()

        onerecipetextmethodframe = DashboardCustomTKframe(master=onerecipeframe,
                                                          width=20, height=40,
                                                          corner_radius=10, fg_color="White",
                                                          bg_color="#00FFFF")
        onerecipetextmethodframe.place(relx=0.08, rely=0.4)

        # the text for the ingredients
        onerecipetextmethod = customtkinter.CTkLabel(master=onerecipetextmethodframe, width=30,
                                                     height=30,
                                                     text="Step 1\nCoat the steak with half of the fajita seasoning\n"
                                                          "and 1 tbsp of the olive oil. Heat a frying pan\n"
                                                          "over a high heat and add the steak, cook for 2 mins\n"
                                                          "on each side for medium rare,\n"
                                                          "remove from the pan and set aside.\n"
                                                          "\nStep 2\nTurn down the heat to medium and add the remaining oil.\n"
                                                          "Tip in the rest of the fajita seasoning, \n"
                                                          "the rice and red peppers. Stir well and cook for 2 mins,\n"
                                                          "Add a splash of water if it starts to stick. \n"
                                                          "Add the lemon juice, stir through half the parsley and season.\n"
                                                          "\nStep 3\nCut the steak into strips. Tip the rice into bowls\n"
                                                          "and top each with the steak and a dollop of yogurt.\n"
                                                          "Scatter with parsley and serve with a lemon wedge.")
        onerecipetextmethod.pack()

