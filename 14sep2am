        onerecipetextnutritonframe.place(relx=0.22, rely=0.1)

        # the text for the nutritional values
        onerecipetextnutriton = customtkinter.CTkLabel(master=onerecipetextnutritonframe, width=30,
                                                       height=30, text="280 Kcal\n26g protein\n"
                                                                       "7g sugars\n26g fats")
        onerecipetextnutriton.pack()

        # the frame for the ingredients
        onerecipetextingredientsframe = DashboardCustomTKframe(master=onerecipeframe,
                                                               width=20, height=40,
                                                               corner_radius=10, fg_color="White",
                                                               bg_color="#00FFFF")
        onerecipetextingredientsframe.place(relx=0.15, rely=0.23)

        # the text for the ingredients
        onerecipetextingredients = customtkinter.CTkLabel(master=onerecipetextingredientsframe, width=30,
                                                          height=30,
                                                          text="600g chicken breast fillets, cut into 2cm pieces\n"
                                                               "2tbsp cornflour\n"
                                                               "5tbsp plain flour\n"
                                                               "1tsp baking powder\n"
                                                               "large pinch of white pepper\n"
                                                               "1 egg, beaten\n"
                                                               "2tbsp sunflower or vegetable oil, for frying\n"
                                                               "2 spring onions, finely sliced, to serve\n"
                                                               "cooked noodles, to serve (optional)\nFor the sauce\n"
                                                               "2tsp cornflour, 2 unwaxed lemons, zested and juiced\n"
                                                               "2tbsp honey 2tbsp soy sauce, \n"
                                                               "2tsp sesame seeds 2tsp sesame oil"

                                                          )
        onerecipetextingredients.pack()

        onerecipetextmethodframe = DashboardCustomTKframe(master=onerecipeframe,
                                                          width=20, height=40,
                                                          corner_radius=10, fg_color="White",
                                                          bg_color="#00FFFF")
        onerecipetextmethodframe.place(relx=0.14, rely=0.52)

        # the text for the ingredients
        onerecipetextmethod = customtkinter.CTkLabel(master=onerecipetextmethodframe, width=30,
                                                     height=30, text="Step 1 To make the sauce\n"
                                                                     "tip the cornflour into a bowl and whisk in\n"
                                                                     "the lemon zest and juice until any lumps disappear. \n"
                                                                     "Add the honey, soy, sesame seeds and\n"
                                                                     "sesame oil and mix again. Set aside."

                                                                     "\nStep 2\n"
                                                                     "Combine the cornflour, flour, baking powder, ¼ tsp salt,\n"
                                                                     "the pepper, beaten egg and 4 tbsp water in a large bowl.\n"
                                                                     "Tip in the chicken pieces, gently tossing to coat."

                                                                     "\nStep 3 Heat the vegetable oil over a high heat \n"
                                                                     "fry the battered chicken pieces in batches \n"
                                                                     "for 6 mins per batch until golden and cooked through.\n"
                                                                     "Remove the cooked chicken and transfer to a plate \n"
                                                                     "or tray lined with kitchen paper."

                                                                     "\nStep 3\n"
                                                                     "When all the chicken has been cooked,\n"
                                                                     "return all of it to the pan, still over a high heat, \n"
                                                                     "pour in the sauce and toss to coat. \n"
