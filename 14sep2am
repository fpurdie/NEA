# will be used to open all workouts
def initialiseallworkouts():
    # the frame for the all workouts page
    global allworkoutsframe
    allworkoutsframe = DashboardCustomTKframe(master=window, width=400, height=700,
                                              corner_radius=0, fg_color="#00FFFF",
                                              bg_color="#00FFFF")
    allworkoutsframe.pack()

    mainframeallworkouts = DashboardCustomTKframe(master=allworkoutsframe, width=400, height=700,
                                                  corner_radius=0, fg_color="#00FFFF",
                                                  bg_color="#00FFFF")
    mainframeallworkouts.pack()

    def oneworkouttoall():
        oneworkoutframe.pack_forget()
        mainframeallworkouts.pack()

    def hammercurlbutton():
        mainframeallworkouts.pack_forget()

        global oneworkoutframe
        oneworkoutframe = DashboardCustomTKframe(master=allworkoutsframe, width=400, height=700,
                                                 corner_radius=10, fg_color="#00FFFF",
                                                 bg_color="#00FFFF")
        oneworkoutframe.pack()

        backtoallworkoutsarrowframe = DashboardCustomTKframe(master=oneworkoutframe, width=45,
                                                             height=40, corner_radius=10,
                                                             fg_color="#00FFFF",
                                                             bg_color="#00FFFF")
        backtoallworkoutsarrowframe.place(relx=0.05, rely=0.005)

